    "methods": {
      "constructor": {
        "details": "Initialize a ParameterStore with a set of names and associated values.",
        "params": {
          "_names": "Names of parameters",
          "_values": "abi-encoded values to assign them"
        }
      },
      "createManyProposals(string[],bytes32[],bytes[])": {
        "details": "Create multiple proposals to set values.",
        "params": {
          "keys": "The keys to set",
          "metadataHashes": "Metadata hashes describing the proposals",
          "values": "The values to set for the keys"
        }
      },
      "createProposal(string,bytes32,bytes)": {
        "details": "Create a proposal to set a value.",
        "params": {
          "key": "The key to set",
          "metadataHash": "A reference to metadata describing the proposal",
          "value": "The value to set"
        }
      },
      "get(string)": {
        "details": "Get the parameter value associated with the given name.",
        "params": { "_name": "The name of the parameter to get the value for" }
      },
      "getAsAddress(string)": {
        "details": "Get the parameter value and cast to `address`",
        "params": { "_name": "The name of the parameter to get the value for" }
      },
      "getAsUint(string)": {
        "details": "Get the parameter value and cast to `uint256`",
        "params": { "_name": "The name of the parameter to get the value for" }
      },
      "init()": {
        "details": "Initialize the contract, preventing any more changes not made through slate governance"
      },
      "setInitialValue(string,bytes32)": {
        "details": "Set a parameter before the ParameterStore has been initialized",
        "params": {
          "_name": "The name of the parameter to set",
          "_value": "The abi-encoded value to assign the parameter"
        }
      },
      "setValue(uint256)": {
        "details": "Execute a proposal to set a parameter. The proposal must have been included in an accepted governance slate.",
        "params": { "proposalID": "The proposal" }
      }
    }
