{
  "name": "e2e-tests",
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "version": "1.0.0",
  "description": "Tests for Panvala using selenium webdriver and cucumber-js",
  "main": "index.js",
  "scripts": {
    "test": "node ./node_modules/.bin/cucumber-js src/features/ --require-module @babel/register --format json:report.json"
  },
  "author": "peter-yinusa",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "chai": "^4.2.0",
    "chromedriver": "^76.0.0",
    "cucumber": "^5.1.0",
    "lodash": "^4.17.15",
    "selenium-webdriver": "^4.0.0-alpha.4"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "husky": "^3.0.3",
    "lint-staged": "^9.2.5",
    "prettier": "^1.18.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write --config ./.prettierrc",
      "git add"
    ]
  }
}
